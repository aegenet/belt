{
  "name": "@aegenet/belt-rows-inflator",
  "description": "Rows Inflator, transforms SQL results into nested objects",
  "version": "999.0.0",
  "keywords": ["sql", "query", "rows", "nested", "transform"],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.mjs",
  "exports": {
    "node": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
    },
    "default": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
    }
  },
  "typings": "./dist/node/bundle.d.ts",
  "author": "Alexandre Genet",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/aegenet/belt/tree/master/packages/belt-rows-inflator",
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {},
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "clean": "node ./../../node_modules/rimraf/bin ./dist && node ./../../node_modules/rimraf/bin ./coverage && node ./../../node_modules/rimraf/bin ./.nyc_output && node ./../../node_modules/rimraf/bin ./node_modules",
    "lint": "node ./../../node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "node ./../../node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx} --fix",
    "test-browser": "node ./../../node_modules/jest/bin/jest --config ./jest.web.config.js",
    "test-node": "node ./../../node_modules/jest/bin/jest --config ./jest.config.js --testTimeout=30000",
    "test-local": "node ./../../node_modules/ts-mocha/bin/ts-mocha src/**/*.spec.ts --timeout 30000",
    "build": "yarn tsc && yarn run build:vite",
    "tsc": "node ./../../node_modules/rimraf/bin build && node ./../../node_modules/typescript/bin/tsc",
    "build:vite": "node ./../../node_modules/rimraf/bin dist && node ./../../node_modules/vite/bin/vite build",
    "generate": "yarn tsc && node ./build/belt-rows-inflator/bench/cli.js"
  }
}
