{
  "name": "@aegenet/belt",
  "description": "The Belt - All In One",
  "version": "999.0.0",
  "main": "./dist/node/belt.bundle.js",
  "browser": "./dist/web/belt.bundle.js",
  "typings": "./dist/node/bundle.d.ts",
  "author": "Alexandre Genet",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/aegenet/belt/tree/master/packages/belt",
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@aegenet/belt-crc8": "file:./../belt-crc8",
    "@aegenet/belt-crc32": "file:./../belt-crc32",
    "@aegenet/belt-promise": "file:./../belt-promise",
    "@aegenet/belt-odeep": "file:./../belt-odeep",
    "@aegenet/belt-ofields": "file:./../belt-ofields",
    "@aegenet/belt-odiff": "file:./../belt-odiff",
    "@aegenet/belt-oclone": "file:./../belt-oclone",
    "@aegenet/belt-hook": "file:./../belt-hook",
    "@aegenet/belt-interpolation": "file:./../belt-interpolation",
    "@aegenet/belt-array-duplicates": "file:./../belt-array-duplicates",
    "@aegenet/belt-array-to-obj": "file:./../belt-array-to-obj",
    "@aegenet/belt-array-stats": "file:./../belt-array-stats",
    "@aegenet/belt-obj-to-array": "file:./../belt-obj-to-array"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "clean": "node ./../../node_modules/rimraf/bin ./dist && node ./../../node_modules/rimraf/bin ./coverage && node ./../../node_modules/rimraf/bin ./.nyc_output && node ./../../node_modules/rimraf/bin ./node_modules",
    "lint": "node ./../../node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "node ./../../node_modules/eslint/bin/eslint ./src/**/*.{js,ts,tsx} --fix",
    "test-browser": "node ./../../node_modules/jest/bin/jest --config ./jest.web.config.js",
    "test-node": "node ./../../node_modules/jest/bin/jest --config ./jest.config.js",
    "test-local": "node ./../../node_modules/ts-mocha/bin/ts-mocha src/**/*.spec.ts",
    "analyze": "node ./../../node_modules/rimraf/bin dist && node ./../../node_modules/webpack/bin/webpack --env production --analyze",
    "build": "yarn run build:webpack",
    "build:webpack": "node ./../../node_modules/rimraf/bin dist && node ./../../node_modules/webpack/bin/webpack --env production"
  }
}
